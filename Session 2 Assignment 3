	public static void main(String[] args){

		int a = 5;
		int b = 7;
		int c = 9;
		
		if (a > b && a > c){
			
			System.out.println(a + " is the highest number.");
			
		}
		
		else if (b > a && b > c){
			
			System.out.println(b + " is the highest number.");
			
		}
		
		else if (c > a && c > b){
			
			System.out.println(c + " is the highest number.");
			
		}
	
	
	}
